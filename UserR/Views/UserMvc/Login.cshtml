@model UserR.Models.User
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>
@using (Html.BeginForm())
{
 <div>
     @Html.LabelFor(model => model.Email, new { @class = "control-label col-md-2" })
     @Html.EditorFor(model => model.Email, new { @class = "control-label col-md-2" })
 </div><br />
    <br />
    <div>
     @Html.LabelFor(model => model.Password, new { @class = "control-label col-md-2" })
     @Html.PasswordFor(model => model.Password, new { @class = "control-label col-md-2" })
 </div>
    <br /><br />
    <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <a class="btn btn-primary" onclick="LoginFunc()">Login</a>
            </div>
        </div>
    <div>
    @Html.ActionLink("Back to List", "Index")
</div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
